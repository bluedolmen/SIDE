<?xml version='1.0' encoding='UTF-8'?>
<component>
	<scope>template</scope>
	<region-id>formPortlet</region-id>
	<source-id>standalonecreateform</source-id>
	<url>/components/form?mimeType={mimeType}&amp;destination={destination}&amp;formId={formId}
	</url>
	<properties>
		<itemKind>type</itemKind>
		<itemId>{itemId}</itemId>
		<mode>create</mode>
		<submitType>json</submitType>
		<showCaption>true</showCaption>
		<showCancelButton>true</showCancelButton>
		<editInline>true</editInline>
		<googleEditable>true</googleEditable>
	</properties>
</component>
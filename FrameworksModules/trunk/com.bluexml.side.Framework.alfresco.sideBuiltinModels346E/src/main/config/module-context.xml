<?xml version='1.0' encoding='ISO-8859-1'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>

<beans>
    <!-- Registration of new models -->
    <bean id="SIDE_ModelExtension_sideBuildInLibraryAlfresco.dictionaryBootstrap" parent="dictionaryModelBootstrap" depends-on="SIDEdictionaryBootstrap">
        <property name="models">
            <list>
                <value>alfresco/module/SIDE_sideBuiltinModels/model/sideBuildInLibraryAlfrescoModel.xml</value>
            </list>
        </property>
		<property name="labels">
            <list>
                <value>alfresco/module/SIDE_sideBuiltinModels/model/messages</value>
            </list>
        </property>
    </bean>
    <!--
    <bean id="SIDE_ModelExtension_sideBuildInLibraryAlfresco_configBootstrap" class="org.alfresco.web.config.WebClientConfigBootstrap" init-method="init">
      <property name="configs">
        <list>
           <value>classpath:alfresco/module/SIDE_sideBuiltinModels/web-client-config-custom.xml</value>
        </list>
      </property>
   </bean>
    -->
   <bean id="SIDE_ModelExtension_sideBuildInLibraryAlfresco_configBootstrap"
		class="com.bluexml.side.Framework.alfresco.hotdeployer.WebClientRepoBootstrap"
		init-method="init">
		<constructor-arg >
			<ref
				bean="SIDE_ModelExtension_sideBuildInLibraryAlfresco_webClientConfigSource" />
		</constructor-arg>
	</bean>
	<bean
		id="SIDE_ModelExtension_sideBuildInLibraryAlfresco_webClientConfigSource"
		parent="repoUrlConfigSource">
		<constructor-arg>
			<list>
				<value>classpath:alfresco/module/SIDE_sideBuiltinModels/web-client-config-custom.xml</value>
				<value>workspace://SpacesStore/${spaces.company_home.childname}/${spaces.dictionary.childname}/app:webclient_extension/cm:SIDE_ModelExtension_sideBuildInLibraryAlfresco_web-client-config-custom.xml</value>
			</list>
		</constructor-arg>
	</bean> 
    <!-- http://wiki.alfresco.com/wiki/Security_and_Authentication#The_default_permission_model_and_simple_extensions -->
</beans>

<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
   <template-type>com/bluexml/standalonecreateform</template-type>
   <components>
      <component>
         <region-id>formPortlet</region-id>
         <url>/components/form?mimeType={mimeType}&amp;destination={destination}&amp;redirect={redirect}&amp;formId={formId}&amp;css={css}&amp;js={js}
         </url>
         <properties>
            <itemKind>type</itemKind>
            <itemId>{itemId}</itemId>
            <mode>create</mode>
            <submitType>{submitType}</submitType>
            <showCaption>true</showCaption>
            <showCancelButton>true</showCancelButton>
            <editInline>true</editInline>
            <googleEditable>true</googleEditable>
         </properties>
      </component>
   </components>
</template-instance>

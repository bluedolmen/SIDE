<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
   <template-type>com/bluexml/standaloneform</template-type>
   <components>
      <component>
         <region-id>formPortlet</region-id>
         <url>/components/form?mimeType={mimeType}&amp;destination={destination}&amp;redirect={redirect}&amp;formId={formId}&amp;css={css}&amp;js={js}
         </url>
         <properties>
            <itemKind>{itemKind}</itemKind>
            <itemId>{itemId}</itemId>
            <mode>{mode}</mode>
            <submitType>{submitType}</submitType>
            <showCaption>{showCaption}</showCaption>
            <showCancelButton>{showCancelButton}</showCancelButton>
            <editInline>{editInline}</editInline>
            <googleEditable>{googleEditable}</googleEditable>
         </properties>
      </component>
   </components>
</template-instance>

<?xml version='1.0' encoding='UTF-8'?>
<template-instance>
   <template-type>com/bluexml/standalonesearchform</template-type>
   <components>
      <component>
         <region-id>title</region-id>
         <url>/components/title/advsearch-title</url>
      </component>
      <component>
         <region-id>standalonesearch</region-id>
         <url>/components/search/advsearch</url>
         <properties>
            <itemKind>type</itemKind>
            <itemId>{itemId}</itemId>
            <mode>edit</mode>
            <formId>search</formId>
            <showCaption>{showCaption}</showCaption>
            <showSubmitButton>{showSubmitButton}</showSubmitButton>
            <showCancelButton>{showCancelButton}</showCancelButton>
         </properties>
      </component>
   </components>
</template-instance>

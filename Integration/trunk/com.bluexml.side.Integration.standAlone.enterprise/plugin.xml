<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
  <extension id="application" point="org.eclipse.core.runtime.applications">
    <application>
      <run class="com.bluexml.side.integration.standalone.enterprise.ApplicationStarterEnterprise" />
    </application>
  </extension>
  <extension
        id="generate"
        point="org.eclipse.core.runtime.applications">
     <application
           cardinality="singleton-global"
           thread="main"
           visible="true">
        <run
              class="com.bluexml.side.integration.standalone.enterprise.StandaloneGenerateApplication">
        </run>
     </application>
  </extension>  
</plugin>


<?xml version='1.0' encoding='ISO-8859-1'?>
<!DOCTYPE beans PUBLIC '-//SPRING//DTD BEAN//EN' 'http://www.springframework.org/dtd/spring-beans.dtd'>
 
<beans>
 
    <bean id="BlueXML_Workflows.workflowBootstrap" parent="workflowDeployer">
		<property name="workflowDefinitions">
			<list>
				<props>
					<prop key="engineId">jbpm</prop>
					<prop key="location">alfresco/extension/BxDS/generated/bpm/startPotts_processdefinition.xml</prop>
					<prop key="mimetype">text/xml</prop>
					<prop key="redeploy">true</prop>
				</props>
			</list>
		</property>
		<property name="models">
			<list>
                <value>alfresco/extension/BxDS/generated/bpm/model-startPotts.xml</value>
			</list>
		</property>
		<property name="labels">
			<list>
                <value>alfresco/extension/BxDS/generated/bpm/workflow-startPotts-messages</value>
			</list>
		</property>
	</bean>
 
</beans>
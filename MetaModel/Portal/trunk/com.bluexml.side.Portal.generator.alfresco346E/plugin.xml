<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="com.bluexml.side.Application.com_bluexml_application_configuration">
      <metamodel
            description="Check this to generate portal models on leading frameworks.&lt;br&gt; This Portal Metamodel allows to model Application Object structure and persistence.&lt;br&gt;SIDE provides the modeler to graphically produce class models and various generators to instantiate these models on leading frameworks."
            id="http://www.kerblue.org/portal/1.0"
            name="KSR200 Portal"
            url="http://www.kerblue.org/portal/1.0">
         <technology
               description="Check this to generate your class models on the Alfresco product line.&lt;br&gt;Alfresco is the leading open source ECM solution.&lt;br&gt;SIDE allows to optimize the configuration of the Alfresco content model, views, forms and reports."
               id="alfresco"
               name="Alfresco"
               url="http://www.alfresco.com">
            <technologyVersion
                  description="Check this to generate class models on the major release 3 of Alfresco.&lt;br&gt;Alfresco 3.x is the leading open source collaborative ECM solution.&lt;br&gt;Additionally to content generation, SIDE provides useful server components to enhance some features of this release."
                  id="alfresco_3.4.6E"
                  version="Alfresco3.4.6 Enterprise">
               <generatorVersion
                     class="com.bluexml.side.portal.generator.alfresco346e.PortalGenerator346e"
                     hidden="visible"
                     id="com.bluexml.side.Portal.generator.alfresco3.46E"
                     version="SIDE Alfresco Portal Generator 3.46E">
                  <option
                        defaultOption="true"
                        key="com.bluexml.side.Portal.generator.alfresco.doclist"
                        label="Alfresco Share document library component">
                     <moduleDependence
                           moduleId="com.bluexml.side.Framework.alfresco.shareDoclist346E"
                           moduleType="zip"
                           technologyVersion="alfresco_3.4.6E"
                           versionMax="1.0.0"
                           versionMin="1.0.0">
                     </moduleDependence>
                     <moduleDependence
                           moduleId="com.bluexml.side.Framework.alfresco.doclist346E"
                           moduleType="amp"
                           technologyVersion="alfresco_3.4.6E"
                           versionMax="1.0.0"
                           versionMin="1.0.0">
                     </moduleDependence>
                  </option>
                  <option
                        defaultOption="true"
                        key="com.bluexml.side.Portal.generator.alfresco.forms"
                        label="document details editing">
                     <moduleDependence
                           moduleId="com.bluexml.side.Framework.alfresco.formProcessor346E"
                           moduleType="amp"
                           technologyVersion="alfresco_3.4.6E"
                           versionMax="1.0.0"
                           versionMin="1.0.0">
                     </moduleDependence>
                  </option>
                  <option
                        defaultOption="false"
                        documentation="add share component to include custom iframe in pages"
                        key="com.bluexml.side.Portal.generator.alfresco.iframe"
                        label="iframe integration">
                     <moduleDependence
                           moduleId="com.bluexml.side.Framework.alfresco.shareIframe"
                           moduleType="zip"
                           technologyVersion="alfresco_3.4.6E"
                           versionMax="1.0.12"
                           versionMin="1.0.12">
                     </moduleDependence>
                  </option>
                  <option
                        defaultOption="false"
                        documentation="&lt;p&gt;This option enable Xform integration in Alfresco share.&lt;/p&gt; &lt;ul&gt; &lt;li&gt;Search forms can be accessible from advanced-search page&lt;/li&gt; &lt;li&gt;Results are displayed in documentlist components instead of share result list&lt;/li&gt; &lt;li&gt;Native advanced-search page is no longer accessible&lt;/li&gt; &lt;li&gt;Native open search displays results using documentlist too&lt;/li&gt; &lt;li&gt;Open search DO NOT WORK ANYMORE outside sites&lt;/li&gt; &lt;/ul&gt;"
                        key="com.bluexml.side.Portal.generator.alfresco.xforms34d"
                        label="XForms Integration">
                     <moduleDependence
                           moduleId="com.bluexml.side.Framework.alfresco.shareSearchResultsExtension346E"
                           moduleType="zip"
                           technologyVersion="alfresco_3.4.6E"
                           versionMax="1.0.0"
                           versionMin="1.0.0">
                     </moduleDependence>
                  </option>
                  <mustBeChecked
                        pluginId="com.bluexml.side.Class.generator.alfresco346E">
                     <optionRef
                           optionId="alfresco.share.extension">
                     </optionRef>
                  </mustBeChecked>
                  <moduleDependence
                        moduleId="com.bluexml.side.Framework.alfresco.sharePortalExtension346E"
                        moduleType="zip"
                        technologyVersion="alfresco_3.4.6E"
                        versionMax="1.0.0"
                        versionMin="1.0.0">
                  </moduleDependence>
                  <moduleDependence
                        moduleId="com.bluexml.side.Framework.alfresco.cmisFacetMap346E"
                        moduleType="amp"
                        technologyVersion="alfresco_3.4.6E"
                        versionMax="1.0.0"
                        versionMin="1.0.0">
                  </moduleDependence>
                  <configurationParameter
                        dataType="String"
                        defaultValue="http://localhost:8080/share"
                        documentation="Set this value to the URL of the Alfresco Share instance on which you want to generate and deploy"
                        key="alfresco.share.url"
                        label="Alfresco share URL">
                  </configurationParameter>
                  <configurationParameter
                        dataType="String"
                        defaultValue="1.0"
                        key="com.bluexml.side.Portal.generator.alfresco.module.version"
                        label="Portal module version">
                  </configurationParameter>
                  <option
                        defaultOption="true"
                        documentation="this must be hidden"
                        hidden="true"
                        key="generator.main"
                        label="main generation">
                  </option>
               </generatorVersion>
            </technologyVersion>
         </technology>
      </metamodel>
   </extension>

</plugin>

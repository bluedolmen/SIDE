<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="com.bluexml.side.modeler.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="com.bluexml.side.form.presentation.formModelWizardID"
            name="%_UI_formModelWizard_label"
            class="com.bluexml.side.form.presentation.FormModelWizard"
            category="com.bluexml.side.modeler.category.ID"
            icon="icons/full/obj16/formModelFile.gif">
         <description>%_UI_formModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="com.bluexml.side.form.presentation.formEditorID"
            name="%_UI_formEditor_label"
            icon="icons/full/obj16/formModelFile.gif"
            extensions="form"
            class="com.bluexml.side.form.presentation.FormEditor"
            contributorClass="com.bluexml.side.form.presentation.FormActionBarContributor">
      </editor>
   </extension>

</plugin>

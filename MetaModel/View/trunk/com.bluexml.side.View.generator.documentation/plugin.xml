<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="com.bluexml.side.Application.com_bluexml_application_configuration">
      <metamodel
            id="http://www.kerblue.org/view/1.0"
            name="KSR300 View"
            url="http://www.kerblue.org/v2/ksr300-view">
         <technology
               description="Generate documentation (in docbook) for the models of the metamodel."
               id="docbook"
               name="docBook"
               url="http://www.docbook.org/">
            <technologyVersion
                  id="docbook_4.x"
                  version="Docbook 4.x">
               <generatorVersion
                     class="com.bluexml.side.view.generator.documentation.ViewDocumentationGenerator"
                     hidden="hidden and used by default"
                     id="com.bluexml.side.View.generator.documentation"
                     version="S-IDE View Documentation Generator 1.0.0">
               </generatorVersion>
            </technologyVersion>
         </technology>
      </metamodel>
   </extension>

</plugin>
